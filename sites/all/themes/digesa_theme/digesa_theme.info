name = Digesa Theme
description = This is a sub-theme of sub-theme Unsemantic Theme of theme Omega 4 (responsive, unsemantic).
version = 1.0
core = 7.x
base theme = unsemantic_theme

; ========================================
; Stylesheets
; ========================================
stylesheets[all][] = css/global.css
stylesheets[all][] = css/admin.css
stylesheets[all and (max-width:750px)][] = css/global-responsive.css

